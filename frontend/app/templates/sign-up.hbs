<div class="form-div">
  <div class='page-header'>
      <h1>Sign Up</h1>
  </div>
  <form class="sign-up-in-form">
    <div class="form-group">
      <label for="exampleInputEmail1">Email address</label>
      {{input type="email" class="form-control" value=email placeholder="Enter email"}}
    </div>
    <div class="form-group">
      <label for="exampleInputPassword1">Password</label>
      {{input type='password' class='form-control' value=password placeholder='Password'}}
    </div>
    <div class="button-holder">
      <button class="btn sign-in-out-btn custom-button" {{action 'signUp'}}> Sign Up</button>
    </div>
  </form>
</div>